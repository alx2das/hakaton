{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6":true
  },
  "extends":["eslint:recommended"],
  "globals": {
    "_": true,
    "$":true,
    "angular":true,
    "ga":true,
    "Ya":true,
    "d3":true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-mixed-spaces-and-tabs": "warn",
    "no-cond-assign": "off",
    "no-console": "error",
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "ignore"
    }]
  }
}